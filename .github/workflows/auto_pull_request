name: pull_request
on:
  pull_request: 
    branches:
      - '?*\-api\/master'
    types:
      closed
jobs:
  pull_request:
    runs-on: ubuntu-latest
    steps:
      - name: Create Pull Request
        id: cpr
        uses: peter-evans/create-pull-request@v1
        with:
          token: ${{ secrets.ACCESS_TOKEN }}
