{
  "name": "product",
  "version": "0.0.0",
  "private": true,
  "lint-staged": {
    "*/*.js": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "scripts": {
    "test": "jest --runInBand --setupFiles dotenv/config --forceExit",
    "precommit": "lint-staged && yarn test",
    "dev:start": "nodemon --exec babel-node bin/www.js",
    "build": "babel ./ -d build --copy-files '*.json'",
    "start": "node ./build/bin/www.js",
    "lint:fix": "eslint . --fix",
    "seed": "babel-node ./db/seeds/product.js"
  },
  "dependencies": {
    "aws-sdk": "^2.580.0",
    "axios": "^0.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "etag": "^1.8.1",
    "express": "~4.16.1",
    "mongoosastic": "https://github.com/kgpyo/mongoosastic",
    "mongoose": "^5.7.12",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^2.0.1"
  }
}
